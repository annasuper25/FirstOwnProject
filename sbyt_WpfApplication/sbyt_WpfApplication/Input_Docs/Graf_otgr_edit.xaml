            <ComboBoxItem Content="2016"/>
            
        </ComboBox>
        <Label Content="Месяц" Height="28" HorizontalAlignment="Left" Margin="176,51,0,0" Name="label5" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="228,51,0,0" Name="comboBox4" VerticalAlignment="Top" Width="54" >
            <ComboBoxItem Content='1'/>
            <ComboBoxItem Content="2"/>
            <ComboBoxItem Content="3"/>
            <ComboBoxItem Content="4"/>
        </ComboBox>
    </Grid>
</Window>
