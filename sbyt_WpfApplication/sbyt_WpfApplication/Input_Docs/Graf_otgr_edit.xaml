        <ComboBox Height="23" HorizontalAlignment="Left" Margin="175,89,0,0" Name="comboBox2" VerticalAlignment="Top" Width="172" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="99,51,0,0" Name="comboBox3" VerticalAlignment="Top" Width="57" >
            <ComboBoxItem Content='2013'/>
